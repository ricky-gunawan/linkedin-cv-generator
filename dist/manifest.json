{
  "manifest_version": 3,
  "name": "linkedin cv generator",
  "version": "1.0",
  "permissions": ["activeTab"],
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/in/*"],
      "js": ["assets/index-CwojflnP.js"],
      "css": ["content.css"]
    }
  ]
}
